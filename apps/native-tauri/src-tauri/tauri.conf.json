{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "koloda",
  "version": "0.0.1",
  "identifier": "koloda",
  "build": {
    "frontendDist": "../../native-tauri-react",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "nx build native-tauri-react"
  },
  "app": {
    "windows": [
      {
        "title": "KOLODA",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
