{
  "sourceRoot": "apps/native-tauri-react/src",
  "projectType": "application",
  "tags": ["type:app", "platform:web"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/native-tauri-desktop"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "build",
        "port": 3000
      },
      "configurations": {
        "development": {
          "buildTarget": "build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "build:production"
        }
      }
    },
    "lingui-extract": {
      "executor": "nx:run-script",
      "options": {
        "script": "lingui extract"
      }
    },
    "lingui-compile": {
      "executor": "nx:run-script",
      "options": {
        "script": "lingui compile"
      }
    }
  }
}
